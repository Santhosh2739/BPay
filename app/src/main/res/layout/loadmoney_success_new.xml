<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/light_blue" />

    <View
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_gravity="center" />
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/tickk"
        android:id="@+id/loadmoney_success_status_img"
        />

    <TextView
        android:id="@+id/loadmoney_success_title_txt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="@string/loadmoney_success_title_txt"
        android:textColor="@color/black"
        android:textSize="25sp" />

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="20dp"
        android:background="@color/white">

        <View
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_gravity="center" />
        <TableRow
            android:layout_weight="1"
            android:background="@color/white">

            <TextView
                android:id="@+id/loadmoney_success_status"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:gravity="start|center"
                android:paddingStart="5dp"
                android:text="@string/status"
                android:textColor="@color/black"
                android:textSize="18sp" />

            <View
                android:layout_width="0.1dp"
                android:layout_height="30dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@color/light_blue" />

            <TextView
                android:id="@+id/loadmoney_success_status_value_txt"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:gravity="start|center"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:textColor="@color/black"
                android:textSize="18sp" />
        </TableRow>

        <TableRow
            android:layout_weight="1"
            android:background="@color/white">

            <TextView
                android:id="@+id/loadmoney_success_paymentID_txt"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:gravity="start|center"
                android:paddingStart="5dp"
                android:text="@string/loadmoney_success_paymentID_txt"
                android:textColor="@color/black"
                android:textSize="18sp" />

            <View
                android:layout_width="0.1dp"
                android:layout_height="30dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@color/light_blue" />

            <TextView
                android:id="@+id/loadmoney_success_paymentID_value_txt"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:gravity="start|center"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:textColor="@color/black"
                android:textSize="18sp" />
        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:layout_gravity="center"
            android:layout_marginTop="2dp"
            android:background="@color/light_blue" />

        <TableRow
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:background="@color/white">

            <TextView
                android:id="@+id/loadmoney_success_post_date_txt"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:gravity="start|center"
                android:paddingStart="5dp"
                android:text="@string/loadmoney_success_post_date_txt"
                android:textColor="@color/black"
                android:textSize="18sp" />

            <View
                android:layout_width="0.1dp"
                android:layout_height="30dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@color/light_blue" />

            <TextView
                android:id="@+id/loadmoney_success_post_date_value_txt"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:gravity="start|center"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:textColor="@color/black"
                android:textSize="18sp" />
        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:layout_gravity="center"
            android:layout_marginTop="2dp"
            android:background="@color/light_blue" />

        <TableRow
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:background="@color/white">

            <TextView
                android:id="@+id/loadmoney_success_result_code_txt"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:gravity="start|center"
                android:paddingStart="5dp"
                android:text="@string/loadmoney_success_result_code_txt"
                android:textColor="@color/black"
                android:textSize="18sp" />

            <View
                android:layout_width="0.1dp"
                android:layout_height="30dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@color/light_blue" />

            <TextView
                android:id="@+id/loadmoney_success_result_code_value_txt"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:gravity="start|center"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:textColor="@color/black"
                android:textSize="18sp" />
        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:layout_gravity="center"
            android:layout_marginTop="2dp"
            android:background="@color/light_blue" />

        <TableRow
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:background="@color/white">

            <TextView
                android:id="@+id/loadmoney_success_transaction_id_txt"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:gravity="start|center"
                android:paddingStart="5dp"
                android:text="@string/loadmoney_success_transaction_id_txt"
                android:textColor="@color/black"
                android:textSize="18sp" />

            <View
                android:layout_width="0.1dp"
                android:layout_height="30dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@color/light_blue" />

            <TextView
                android:id="@+id/loadmoney_success_transaction_id_value_txt"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:gravity="start|center"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:textColor="@color/black"
                android:textSize="18sp" />
        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:layout_gravity="center"
            android:layout_marginTop="2dp"
            android:background="@color/light_blue" />


        <TableRow
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:background="@color/white">

            <TextView
                android:id="@+id/loadmoney_success_auth_txt"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:gravity="start|center"
                android:paddingStart="5dp"
                android:text="@string/loadmoney_success_auth_txt"
                android:textColor="@color/black"
                android:textSize="18sp" />

            <View
                android:layout_width="0.1dp"
                android:layout_height="30dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@color/light_blue" />

            <TextView
                android:id="@+id/loadmoney_success_auth_value_txt"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:gravity="start|center"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:textColor="@color/black"
                android:textSize="18sp" />
        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:layout_gravity="center"
            android:layout_marginTop="2dp"
            android:background="@color/light_blue" />

        <TableRow
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:background="@color/white">

            <TextView
                android:id="@+id/loadmoney_success_track_id_txt"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:gravity="start|center"
                android:paddingStart="5dp"
                android:text="@string/loadmoney_success_track_id_txt"
                android:textColor="@color/black"
                android:textSize="18sp" />

            <View
                android:layout_width="0.1dp"
                android:layout_height="30dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@color/light_blue" />

            <TextView
                android:id="@+id/loadmoney_success_track_id_value_txt"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:gravity="start|center"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:textColor="@color/black"
                android:textSize="18sp" />
        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:layout_gravity="center"
            android:layout_marginTop="2dp"
            android:background="@color/light_blue" />

        <TableRow
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:background="@color/white">

            <TextView
                android:id="@+id/loadmoney_success_ref_no_txt"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:gravity="start|center"
                android:paddingStart="5dp"
                android:text="@string/loadmoney_success_ref_no_txt"
                android:textColor="@color/black"
                android:textSize="18sp" />

            <View
                android:layout_width="0.1dp"
                android:layout_height="30dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@color/light_blue" />

            <TextView
                android:id="@+id/loadmoney_success_ref_no_value_txt"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:gravity="start|center"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:textColor="@color/black"
                android:textSize="18sp" />
        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:layout_gravity="center"
            android:layout_marginTop="2dp"
            android:background="@color/light_blue" />

        <View
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_gravity="center" />

        <Button
            android:id="@+id/loadmoney_success_close_btn"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_gravity="center"
            android:layout_marginLeft="80dp"
            android:layout_marginRight="80dp"
            android:layout_weight="1"
            android:background="@color/light_blue"
            android:text="@string/registration_success_close_btn"
            android:textAllCaps="true"
            android:textColor="@color/white"
            android:textSize="18sp" />
    </TableLayout>
</LinearLayout>